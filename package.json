{
  "name": "slipshow",
  "repository": {
    "type": "git",
    "url": "https://github.com/panglesd/slipshow-vscode.git"
  },
  "publisher": "Slipshow",
  "displayName": "slipshow",
  "description": "An extension to write, preview and compile slipshow presentations",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension_compiled.js",
  "browser": "./web_ide.js",
  "contributes": {
    "commands": [
      {
        "command": "slipshow.preview",
        "title": "Preview Slipshow"
      },
      {
        "command": "slipshow.slipshow",
        "title": "Compile Slipshow"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "constants-browserify": "^1.0.0",
    "eslint": "^8.56.0",
    "tty-browserify": "^0.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2"
  }
}
